function "public"."inventory_held_by_customer(integer)" {
  text = """-

DECLARE
    v_customer_id INTEGER;
BEGIN

  SELECT customer_id INTO v_customer_id
  FROM rental
  WHERE return_date IS NULL
  AND inventory_id = p_inventory_id;

  RETURN v_customer_id;
END 
"""
  returnType = integer
  arguments = <
    {
      name = p_inventory_id
      type = integer
      mode = IN
    }

  >
  language = plpgsql
  owner = postgres
}

